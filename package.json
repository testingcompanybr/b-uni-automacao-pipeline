{
  "name": "automacao-login-selenium-cucumber",
  "version": "1.0.0",
  "description": "Automação de testes de login com Selenium, Cucumber e TypeScript",
"scripts": {
  "test": "cucumber-js --require-module ts-node/register --require Steps/**/*.ts --require Support/**/*.ts",
  "test:mobile": "npm run test -- --tags '@mobile'",
  "test:web": "npm run test -- --tags 'not @mobile'",
  "test:pix": "npm run test -- --tags '@pix'",
  "test:custom": "npm run test -- --tags"
},
  "devDependencies": {
    "@cucumber/cucumber": "^10.9.0",
    "@cucumber/messages": "^27.2.0",
    "@types/chai": "^4.3.5",
    "@types/node": "^20.17.50",
    "@types/selenium-webdriver": "^4.1.28",
    "@wdio/appium-service": "^9.14.0",
    "allure-cucumberjs": "^3.2.2",
    "appium": "^2.18.0",
    "appium-uiautomator2-driver": "^4.2.3",
    "chai": "^4.3.7",
    "chromedriver": "^136.0.0",
    "selenium-webdriver": "^4.21.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "appium": "^2.0.0",
    "webdriverio": "^8.0.0"
  }
}
